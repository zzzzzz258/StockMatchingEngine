<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.duke.ece568.em.server.AccountMapper">
  <insert id="insertAccount">
    insert into account (account_id, balance) values (#{accountId}, #{balance});
  </insert>


  
  <select id="selectAll">
    select * from account
  </select>
</mapper>


